#include <iostream> 
using namespace std;

int main()
{
    cout << "[1] русский" << endl;
    cout << "[2] английский" << endl;
    int language;
    cin >> language;
    
    switch (language)
    {
        case 1:
        cout << "1) Железо" << endl;
        cout << "2) капитан" << endl;
        cout << "3) удар" << endl;
        cout << "4) кулак" << endl;
        cout << "5) лук" << endl;
        cout << "6) человек" << endl;
        cout << "7) паук" << endl;
        cout << "8) сорвиголова" << endl;
