        cout << "10) летучая мышь" << endl;
        cout << "11) охотник" << endl;
        cout << "12) вспышка" << endl;
        cout << "13) фонарь" << endl;
        cout << "14) лига" << endl;
        cout << "15) справедливость." << endl;
        cout << "Введите номер:";
        
        int number1;
        cin >> number1;
        switch (number1)
        {
            case 1:
            cout << "Iron";
            break;
            
            case 2:
            cout << "captain";
            break;
            
            case 3:
            cout << "punch";
            break;
            
            case 4:
            cout << "fist";
            break;
            
            case 5:
            cout << "bow";
            break;
            
            case 6:
            cout << "human";
            break;
            
            case 7:
            cout << "spider";
            break;
            
            case 8:
            cout << "daredevil";
            break;
            
            case 9:
            cout << "avenger";
            break;
            
            case 10:
            cout << "bat";
            break;
            
            case 11:
            cout << "hunter";
            break;
            
            case 12:
            cout << "flash";
            break;
            
            case 13:
            cout << "lantern";
            break;
            
            case 14:
            cout << "league";
            break;
            
            case 15:
            cout << "justice";
            break;
            
            default:
            cout << "не тот номер" << endl;
            break;
        }
        break;
        
        case 2:
        cout << "1) Iron" << endl;
        cout << "2) captain" << endl;
        cout << "3) punch" << endl;
        cout << "4) fist" << endl;
        cout << "5) bow" << endl;
        cout << "6) human" << endl;
        cout << "7) spider" << endl;
        cout << "8) daredevil" << endl;
        cout << "9) avenger" << endl;
        cout << "10) bat" << endl;
        cout << "11) hunter" << endl;
        cout << "12) flash" << endl;
        cout << "13) lantern" << endl;
        cout << "14) league" << endl;
        cout << "15) justice." << endl;
        cout << "Введите номер:";
        
        int number2;
        cin >> number2;
        switch (number2)
        {
            case 1:
            cout << "Железо";
            break;
            
            case 2:
            cout << "капитан";
            break;
            
            case 3:
            cout << "удар";
            break;
            
            case 4:
            cout << "кулак";
            break;
            
            case 5:
            cout << "лук";
            break;
            
            case 6:
            cout << "человек";
            break;
            
            case 7:
            cout << "паук";
            break;
            
            case 8:
            cout << "сорвиголова";
            break;
            
            case 9:
            cout << "мститель";
            break;
            
            case 10:
            cout << "летучая мышь";
            break;
            
            case 11:
            cout << "охотник";
            break;
            
            case 12:
            cout << "вспышка";
            break;
            
            case 13:
            cout << "фонарь";
            break;
            
            case 14:
            cout << "лига";
            break;
            
            case 15:
            cout << "справедливость";
            break;
            
            default:
            cout << "не тот номер" << endl;
            break;
        }
        break;
        default:
            cout << "не тот номер" << endl;
            break;
    }
    return 0;
